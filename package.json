{
  "name": "hanchor",
  "version": "1.0.4",
  "description": "Autogenerate links to header tags from header text",
  "license": "MIT",
  "repository": "rod/hanchor",
  "homepage": "https://github.com/rod/hanchor",
  "author": "Rodney Dennis <rodneydennisjr@gmail.com> (https://rod.today)",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "babel src/hanchor.js -w -o hanchor.js",
    "build:expanded": "babel src/hanchor.js -o hanchor.js",
    "build:compressed": "babel src/hanchor.js -o hanchor.min.js --presets=babili",
    "build": "npm run build:expanded & npm run build:compressed",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": "umd"
        }
      ],
      "stage-0"
    ]
  },
  "main": "hanchor.min.js",
  "keywords": [
    "a",
    "anchor",
    "h",
    "header",
    "link",
    "slug",
    "slugify",
    "string",
    "tag",
    "unicode",
    "url",
    "utf-8",
    "utf"
  ],
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "xo": "^0.17.0"
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "space": true,
    "extends": "airbnb-base"
  },
  "dependencies": {
    "babel-loader": "^6.4.0"
  }
}
